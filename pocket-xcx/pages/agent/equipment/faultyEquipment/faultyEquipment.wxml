<view class="wrap">
    <view class="content" wx:if="{{!isNoData}}">
        <view class="l-list">
            <view class="m-item" wx:for='{{dataList}}' wx:key='{{index}}'>
                <navigator url='/pages/agent/equipment/handleTrouble/handleTrouble?posCode={{item.posCode}}'>
                    <view>{{item.posName}}</view>
                    <view class="s-inf">{{item.detailAddress}}</view>
                    <text class="right"><text class="red">{{item.restNum}}</text>/{{item.capacity}}</text>
                    <text class="more" style="margin-top:40rpx"></text>
                </navigator>
            </view>
        </view>
    </view>
    <view class="no-data" wx:if="{{isNoData}}"></view>
</view>