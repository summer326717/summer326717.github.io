<view class="wrap">
    <view class="content" wx:if="{{!isNoData}}">
        <view class="content">
            <view class="l-list">
                <view class="m-item" wx:for='{{dataList}}' wx:key='{{index}}'>
                    <navigator url='/pages/agent/equipment/equipmentDetail/equipmentDetail?posCode={{item.posCode}}'>
                        <view>{{item.posName?item.posName:item.posCode}}<text decode="{{true}}">&nbsp;</text></view>
                        <view class="s-inf">{{item.detailAddress?item.detailAddress:''}}<text decode="{{true}}">&nbsp;</text></view>
                        <text class="right" style="margin-top:25rpx">{{item.restNum}}/{{item.capacity?item.capacity:0}}</text>
                        <text class="more" style="margin-top:40rpx"></text>
                    </navigator>
                </view>
            </view>
        </view>
    </view>
    <view class="no-data" wx:if="{{isNoData}}"></view>
</view>